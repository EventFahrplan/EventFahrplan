<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="StyleableEditTextPreference">
        <attr name="android:hint" format="string" />
        <attr name="android:maxLines" format="integer" />
        <attr name="android:singleLine" format="boolean" />
    </declare-styleable>

    <declare-styleable name="ValidateableEditTextPreference">
        <attr name="validationType">
            <enum name="unknown" value="0" />
            <enum name="url" value="1" />
            <enum name="engelsystemUrl" value="2" />
        </attr>
        <attr name="urlTypeFriendlyName" format="string" />
    </declare-styleable>

</resources>
